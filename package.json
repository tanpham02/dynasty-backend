{
  "name": "dynasty-backend",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "dev": "env-cmd -f .env.development nodemon",
    "run:staging": "yarn build && env-cmd -f .env.staging node ./build/index.js",
    "run:pro": "yarn build && env-cmd -f .env.production node ./build/index.js",
    "build": "rimraf ./build && tsc && tsc-alias",
    "seed": "env-cmd -f .env.$NODE_ENV ts-node -r tsconfig-paths/register src/seeds/index.ts",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "prettier": "prettier --check .",
    "prettier:fix": "prettier --write ."
  },
  "_moduleAliases": {
    "@app/configs": "./src/configs",
    "@app/connection": "./src/connection",
    "@app/constants": "./src/constants",
    "@app/controllers": "./src/controllers",
    "@app/exception": "./src/exception",
    "@app/middlewares": "./src/middlewares",
    "@app/models": "./src/models",
    "@app/routes": "./src/routes",
    "@app/services": "./src/services",
    "@app/types": "./src/types",
    "@app/utils": "./src/utils",
    "@app/events": "./src/events"
  },
  "license": "MIT",
  "dependencies": {
    "@types/multer": "1.4.9",
    "@types/swagger-jsdoc": "6.0.1",
    "@types/swagger-ui-express": "4.1.3",
    "axios": "1.6.8",
    "bcrypt": "5.1.1",
    "cookie-parser": "1.4.6",
    "cors": "2.8.5",
    "cron": "3.2.0",
    "dotenv": "16.3.1",
    "env-cmd": "10.1.0",
    "express": "4.18.2",
    "follow-redirects": "1.15.6",
    "google-auth-library": "9.7.0",
    "helmet": "8.0.0",
    "https": "1.0.0",
    "iter-ops": "3.1.1",
    "jsonwebtoken": "9.0.1",
    "jwt-decode": "4.0.0",
    "lodash": "4.17.21",
    "module-alias": "2.2.3",
    "moment": "2.29.4",
    "moment-timezone": "0.5.43",
    "mongoose": "7.4.3",
    "multer": "1.4.5-lts.1",
    "nodemailer": "6.9.11",
    "socket.io": "4.8.0",
    "swagger-jsdoc": "6.2.8",
    "swagger-ui-express": "5.0.0",
    "twilio": "5.4.0"
  },
  "devDependencies": {
    "@types/bcrypt": "5.0.0",
    "@types/cookie-parser": "1.4.6",
    "@types/cors": "2.8.13",
    "@types/express": "5.0.0",
    "@types/follow-redirects": "1.14.4",
    "@types/jsonwebtoken": "9.0.3",
    "@types/lodash": "4.14.202",
    "@types/moment-timezone": "0.5.30",
    "@types/morgan": "1.9.4",
    "@types/node": "20.4.5",
    "@types/nodemailer": "6.4.14",
    "@typescript-eslint/eslint-plugin": "6.13.2",
    "@typescript-eslint/parser": "6.13.2",
    "concurrently": "8.2.0",
    "eslint": "8.55.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-prettier": "5.0.1",
    "morgan": "1.10.0",
    "nodemon": "3.0.1",
    "prettier": "3.1.0",
    "rimraf": "5.0.5",
    "ts-node": "10.9.1",
    "tsc-alias": "1.8.8",
    "tsconfig-paths": "4.2.0",
    "typescript": "5.1.6"
  },
  "author": {
    "name": "Tan Van Pham",
    "email": "phamvantan1311@gmail.com"
  }
}
